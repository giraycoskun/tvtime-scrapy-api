  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Pytest Module",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "justMyCode": false
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["src.main:app", "--reload", "--port=8000"],
      "jinja": true,
      "justMyCode": true
    }
  ]
}
